{
  "name": "swiftsend",
  "version": "1.1.7",
  "private": true,
  "scripts": {
    "start:build": "tsc",
    "start:dev": "npx nodemon src/index.ts --watch",
    "start": "node dist/src/index.js",
    "start:release": "git tag v$(node -p \"require('./package.json').version\") && git push origin v$(node -p \"require('./package.json').version\")",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org arijit-chhatui --project swiftsend ./dist && sentry-cli sourcemaps upload --org arijit-chhatui --project swiftsend ./dist"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.777.0",
    "@sentry/cli": "^2.43.0",
    "@sentry/node": "^9.24.0",
    "@sentry/profiling-node": "^9.35.0",
    "@sentry/tracing": "^7.120.3",
    "@socket.io/admin-ui": "^0.5.1",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "lint": "^1.1.2",
    "mongodb": "^6.18.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.1",
    "nodemailer": "^6.10.0",
    "puppeteer": "^24.17.1",
    "radash": "^12.1.0",
    "redis": "^4.7.0",
    "sharp": "^0.34.2",
    "socket.io": "^4.8.1",
    "stripe": "^18.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@types/nodemailer": "^6.4.17",
    "@types/socket.io": "^3.0.2",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
