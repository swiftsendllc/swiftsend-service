{
  "name": "swiftsend",
  "version": "1.1.4",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "npx nodemon src/index.ts --watch",
    "start": "node dist/src/index.js",
    "release": "git tag v$(node -p \"require('./package.json').version\") && git push origin v$(node -p \"require('./package.json').version\")",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org arijit-chhatui --project swiftsend ./dist && sentry-cli sourcemaps upload --org arijit-chhatui --project swiftsend ./dist"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.670.0",
    "@sentry/cli": "^2.41.1",
    "@sentry/node": "^8.52.1",
    "@sentry/profiling-node": "^8.52.0",
    "@sentry/tracing": "^7.120.3",
    "@socket.io/admin-ui": "^0.5.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.20.0",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "lint": "^0.8.19",
    "mongodb": "^6.8.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "radash": "^12.1.0",
    "redis": "^4.7.0",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "stripe": "^17.7.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.5.4",
    "@types/socket.io": "^3.0.2",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
